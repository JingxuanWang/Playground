{
	"manifest_version": 2,

	"name": "Surf Logger",
	"description": "This extension logs most frequent visited sites",
	"version": "0.0.1",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"history",
		"webRequest",
		"webNavigation"
	],
	"background": {
		"scripts": [
			"SurfLogger.js"
		]
	}
}
